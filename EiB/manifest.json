{

  "manifest_version": 2,
  "name": "EiB",
  "version": "2.3",

  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "https://*.google.com/",
    "https://dictionaryapi.com/",
    "https://api.spotify.com/"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self';",
  "chrome_url_overrides": {
    "newtab": "popup/assistant.html"
  },
  "browser_action": {
    "default_icon": "img/ear.jpg",
    "default_title": "Ear in the Browser"
  }
}